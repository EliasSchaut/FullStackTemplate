    "class-validator": "^0.14.1",
    "graphql": "^16.10.0",
    "helmet": "^7.2.0",
    "jest": "^29.7.0",
    "joi": "^17.13.3",
    "nestjs-i18n": "^10.5.0",
    "nestjs-prisma": "^0.23.0",
    "nuxt": "^3.15.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma-error-enum": "^0.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",

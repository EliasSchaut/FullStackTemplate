{
  "name": "personal-full-template",
  "description": "My personal for full stack projects",
  "author": "EliasSchaut",
  "private": true,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EliasSchaut/FullTemplate.git"
  },
  "scripts": {
    "start:client": "node .output/server/index.mjs",
    "start:server": "nest start",
    "dev:client": "nuxt dev",
    "dev:server": "nest start --debug --watch",
    "test:server": "jest --config=jest.config.json",
    "build": "npm run build:db && npm run build:server && npm run build:client",
    "build:server": "nest build",
    "build:client": "nuxt build",
    "build:db": "npx prisma generate",
    "migrate:db": "npx prisma migrate dev --schema=./schema.prisma --name dev",
    "format": "prettier --write \\\"server/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "pinia": "^2.3.0"
  },
  "devDependencies": {
    "@apollo/server": "^4.11.2",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/testing": "^10.4.15",
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/image": "^1.8.1",
    "@nuxtjs/apollo": "^5.0.0-alpha.14",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/i18n": "^9.1.1",
    "@nuxtjs/sitemap": "^6.1.5",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.1",
    "@pinia/nuxt": "^0.8.0",
    "@prisma/client": "^5.22.0",
    "@tailwindcss/forms": "^0.5.9",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "autoprefixer": "^10.4.20",
    "bcrypt": "^5.1.1",
